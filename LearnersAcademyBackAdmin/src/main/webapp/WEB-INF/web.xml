  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Connectivity</servlet-name>
  	<url-pattern>/connectivity</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
